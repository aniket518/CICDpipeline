pipeline{  
agent any 
stages{ 
	stage('Build Application') { 
		steps { 
			bat 'mvn clean install' 
		} 
	} 
	stage('Deploy to CloudHub') { 
		environment { 
			ANYPOINT_CREDENTIALS = credentials(' anypointplatformcredentials ') 
		} 
	steps { 
		echo 'Deploying mule project due to the latest code commit…' 
		echo 'Deploying to the configured environment….' 
		bat 'mvn clean deploy -DmuleDeploy  -DmuleDeploy -DPLATFORM_USERNAME=${ANYPOINT_CREDENTIALS_USR} -DPLATFORM_PASSWORD=${ANYPOINT_CREDENTIALS_PSW} -Danypoint.platform.client_id=e81e94ecd8cc4e4485fac0e951c6b0d0 -Danypoint.platform.client_secret=f689692BAbf44F34837B8BD1d763816A'
		}  
	} 
} 
} 